tes/src/slices/index.js",[],"/Users/void/CodeHub/playgrounds/mini-currency-rates/src/slices/ratesReducer.js",["59"],"import { createAsyncThunk, createSlice } from '@reduxjs/toolkit';\nimport axios from 'axios';\nimport _ from 'lodash';\n\nexport const getRates = createAsyncThunk(\n  'currency/getRates',\n  async defaultCurrency => {\n    const API_URL = `https://api.exchangeratesapi.io/latest?base=${defaultCurrency}`;\n    const { data } = await axios.get(API_URL);\n    return data;\n  },\n);\n\nconst { actions, reducer } = createSlice({\n  name: 'currency',\n  initialState: {\n    base: 'USD',\n    favorites: ['JPY', 'EUR', 'PLN'],\n    amount: 1,\n    rates: {},\n  },\n  reducers: {\n    updateAmount(state, { payload }) {\n      console.log('payload ->', payload); // eslint-disable-line no-console\n      state.amount = payload;\n    },\n    updateBase(state, { payload }) {\n      state.base = payload;\n    },\n    toggleFavorites(state, { payload }) {\n      const { favorites } = state;\n      if (favorites.includes(payload)) {\n        state.favorites = favorites.filter(el => el !== payload);\n      } else {\n        state.favorites.push(payload);\n      }\n    },\n  },\n  extraReducers: {\n    [getRates.fulfilled]: (state, { payload }) => {\n      const { rates } = payload;\n      state.rates = { ...rates };\n    },\n  },\n});\n\nexport default reducer;\nexport const { updateAmount, updateBase, toggleFavorites } = actions;\nexport const ratesSelector = state => state.currency.rates\n// export const rateSelector = state =>{ \n//   const {currency: {favorites, rates }} = state;\n//   const sortedRates = _.orderBy(rates, favorites, 'desc')\n//   console.log('sortedRates ->', sortedRates); // eslint-disable-line no-console\n// }\nexport const baseSelector = state => state.currency.base;\nexport const favoritesSelector = state => state.currency.favorites;\nexport const amountSelector = state => state.currency.amount;\n","/Users/void/CodeHub/playgrounds/mini-currency-rates/src/components/App.jsx",[],"/Users/void/CodeHub/playgrounds/mini-currency-rates/src/routes.js",[],"/Users/void/CodeHub/playgrounds/mini-currency-rates/src/components/Dashboard.jsx",[],"/Users/void/CodeHub/playgrounds/mini-currency-rates/src/components/Settings.jsx",[],"/Users/void/CodeHub/playgrounds/mini-currency-rates/src/components/Header.jsx",[],"/Users/void/CodeHub/playgrounds/mini-currency-rates/src/components/Footer.jsx",[],"/Users/void/CodeHub/playgrounds/mini-currency-rates/src/components/Form.jsx",["60","61"],"/Users/void/CodeHub/playgrounds/mini-currency-rates/src/components/Table.jsx",[],"/Users/void/CodeHub/playgrounds/mini-currency-rates/src/components/Select.jsx",["62"],"import { useDispatch, useSelector } from 'react-redux';\nimport React, { useState, useEffect } from 'react';\nimport styled from 'styled-components';\n\nimport {\n  baseSelector,\n  getRates,\n  ratesSelector,\n  updateBase,\n} from '../slices/ratesReducer';\n\nconst StyledForm = styled.form`\n  padding: 0.5em;\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  border: none;\n  // width: 100%;\n  border-radius: 10px;\n  box-shadow: 0 5px 5px rgba(0, 0, 0, 0.2), 0 0 10px rgba(0, 0, 0, 0.2);\n\n  background: white;\n`;\n\nconst StyledText = styled.p`\n  display: flex;\n  align-items: center;\n  font-size: 1.2rem;\n  color: cadetblue;\n  padding-left: 1rem;\n`;\n\nconst StyledSelect = styled.select`\n  &:focus {\n    outline: none;\n  }\n  padding-left: 0.5rem;\n  font-size: 1.2rem;\n  border: none;\n  display: flex;\n  option {\n    background: white;\n    display: flex;\n    white-space: pre;\n    min-height: 1rem;\n  }\n  cursor: pointer;\n`;\nconst Label = styled.label`\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  margin: 0 0.5rem;\n`;\nconst Select = () => {\n  const dispatch = useDispatch();\n  const baseCurrency = useSelector(baseSelector);\n  const rates = useSelector(ratesSelector);\n\n useEffect(() => {\n    const fetchData = async () => {\n      await dispatch(getRates(baseCurrency)).catch(err =>\n        console.error(err),\n      );\n    };\n    fetchData();\n  }, [dispatch, baseCurrency]);\n\n  const handleChange = ({target}) => {\n    const { value } = target\n    dispatch(updateBase(value));\n  };\n\n  return (\n    <StyledForm >\n      <Label>\n        <StyledText>Select currency:</StyledText>\n        <StyledSelect value={baseCurrency} onChange={handleChange}>\n          {Object.keys(rates).map((curr, i) => (\n            <option key={i} value={curr}>\n              {curr}\n            </option>\n          ))}\n        </StyledSelect>\n      </Label>\n      {/* <Button type=\"submit\">Get!</Button> */}\n    </StyledForm>\n  );\n};\n\nexport default Select;\n",{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","severity":1,"message":"68","line":3,"column":8,"nodeType":"69","messageId":"70","endLine":3,"endColumn":9},{"ruleId":"67","severity":1,"message":"68","line":6,"column":8,"nodeType":"69","messageId":"70","endLine":6,"endColumn":9},{"ruleId":"67","severity":1,"message":"71","line":34,"column":7,"nodeType":"69","messageId":"70","endLine":34,"endColumn":13},{"ruleId":"67","severity":1,"message":"72","line":2,"column":17,"nodeType":"69","messageId":"70","endLine":2,"endColumn":25},"no-native-reassign",["73"],"no-negated-in-lhs",["74"],"no-unused-vars","'_' is defined but never used.","Identifier","unusedVar","'Button' is assigned a value but never used.","'useState' is defined but never used.","no-global-assign","no-unsafe-negation"]